<% layout("/layouts/boilerplate")%>
<body>
    <h3>All Data are here..</h3>

    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
        <% for(let list of lists) { %>
            <div class="card col" style="width: 25rem;">
                <a href="/listings/<%=list._id%>" class="card col">
                    <img src="<%= list.image %>" class="card-image-top" alt=" Error">
                <div class="card-body">
                    <p class="card-text"> <b> <%=list.title%> </b><br>
                    &#8377; <%= list.price.toLocaleString("en-IN") %> /night</p>
                </div>
                </a>
            </div>
            <% } %>
    </div>


</body>